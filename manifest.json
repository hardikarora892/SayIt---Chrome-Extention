
{
  "manifest_version": 3,
  "name": "Quote Suggester",
  "version": "1.0",
  "description": "Shows a motivational quote every time you open the browser.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
